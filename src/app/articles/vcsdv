              <div class="form-group">
                <div class="col-lg-offset-2 col-sm-10">
                  <div class="pull-right">
                    <button onclick="replicate()" class="btn btn-info"><i class="fa fa-plus"></i> add</button>
                    <button onclick="removePresc(this.parentNode.parentNode.parentNode.parentNode)" class="btn btn-danger"><i class="fa fa-remove"></i> remove</button>
                  </div></div></div>
            </div>
