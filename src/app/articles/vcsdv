          <script type="text/javascript">
            function replicate()
            {
              //alert('repla');
              var presc = document.getElementById("con");
              var itm = presc.lastElementChild;
              //console.log(itm)
              // Copy the <li> element and its child nodes
              var cln = itm.cloneNode(true);
              var newid = parseInt((itm.id).split("-")[1]) + 1 ;
              cln.id = "item-" + newid;

              $(cln).find('input[type="text"]').val('');
              $(cln).find('input[type="textarea"]').val('');

              $(cln).find('input[type="file"]').val('');
              $(cln).find("#imgurl").val('');


              presc.appendChild(cln);
            }
            2
            function removePresc(node)
            {

              if(node.parentNode.childNodes.length>3){

                node.parentNode.removeChild(node);
              }
            }

          </script>

          <div id="con">
            <div id="item-1">

              <div class="form-group">
                <label class="control-label col-sm-2" for="article_edit_transcript">Question:</label>
                <div class="col-sm-10">
                  <textarea id="t_question" class="form-control" placeholder="Enter Transcript question" name="question" [(ngModel)]="model.question"></textarea>
                </div>
              </div>

              <div class="form-group">
